  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/draftjs-to-html": "^0.8.1",
    "@types/lodash": "^4.14.194",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26"
